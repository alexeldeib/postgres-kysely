{
  "name": "postgres-kysely",
  "repository": "https://github.com/vercel/examples.git",
  "license": "MIT",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "prisma generate && next dev --turbo",
    "build": "prisma generate && prisma db push && prisma db seed && next build",
    "start": "next start",
    "lint": "next lint",
    "diff": "prisma migrate diff --from-schema-datasource ./src/prisma/schema.prisma --to-schema-datamodel ./src/prisma/schema.prisma --script",
    "storybook": "storybook dev -p 3000",
    "build-storybook": "storybook build"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./src/lib/seed.ts"
  },
  "dependencies": {
    "@datastructures-js/trie": "^4.2.2",
    "@vercel/postgres": "0.1.3",
    "@vercel/postgres-kysely": "0.1.0-canary.27",
    "an-array-of-english-words": "^2.0.0",
    "autoprefixer": "10.4.14",
    "bulma": "^0.9.4",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "kysely": "^0.24.2",
    "ms": "^2.1.3",
    "next": "13.3.2",
    "postcss": "8.4.22",
    "prisma": "^4.13.0",
    "prisma-kysely": "^1.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.62.1",
    "seedrandom": "^3.0.5",
    "tailwindcss": "3.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.9",
    "@storybook/addon-interactions": "^7.0.9",
    "@storybook/addon-links": "^7.0.9",
    "@storybook/blocks": "^7.0.9",
    "@storybook/nextjs": "^7.0.9",
    "@storybook/react": "^7.0.9",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/ms": "^0.7.31",
    "@types/node": "^20.1.0",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "@types/seedrandom": "^3.0.5",
    "eslint-plugin-storybook": "^0.6.12",
    "storybook": "^7.0.9",
    "turbo": "^1.9.3"
  }
